<p style="color: green"><%= notice %></p>

<h1>Plans selecteds</h1>

<table class="table">
  <thead>
    <tr>
      <th scope="col">Id</th>
      <th scope="col">User</th>
      <th scope="col">Plan Selected</th>
      <th scope="col">Duration</th>
      <th scope="col">Day used</th>
      <th scope="col">Activated</th>
      <th scope="col">First time</th>
    </tr>
  </thead>
  <tbody>
  <div id="plans_selecteds">
    <% @plans_selecteds.each do |plans_selected| %>
     <tr>
      <%= render plans_selected %>
     </tr>
    <% end %>
  </div>
 
  </tbody>
</table>



